{
  "version": 2,
  "buildCommand": "npm install && DISABLE_ESLINT_PLUGIN=true npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/test.html",
      "dest": "/test.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 